<p style="color: green"><%= notice %></p>

<h1>Friends List</h1>


<table class=table>
  <thread>
    <tr>
      <th>first_name</th>
      <th>last_name</th>
      <th>email</th>
      <th>phone</th>
      <th>twitter</th>
      <th colspan="3"></th>
    </tr>
  </thread>

  

    <tbody>
      <% @friends.each do |friend| %>
        <tr>
          <td><%= friend.first_name %></td>
          <td><%= friend.last_name %></td>
          <td><%= friend.email %></td>
          <td><%= friend.phone %></td>
          <td><%= friend.twitter %></td>
        </tr>
      <% end %>
    </tbody>
  </table>



